{
    "embeddings": {
        "words": {
            "dimension": 200,
            "pretrained_file": "/net/efs/data/dlfa/glove/glove.6B.100d.txt.gz",
            "fine_tune": false,
            "project": true,
            "dropout": 0.0
        },
        "characters": {
            "dimension": 16,
            "dropout": 0.0
        }
    },
    "model_class": "GatedAttentionReader",
    "cloze_token": "xxxxx",
    "num_word_characters": 10,
    "model_serialization_prefix": "models/multiple_choice_qa/gareader_wdw",
    "num_gated_attention_layers": 3,
    "tokenizer": {
        "type": "words and characters"
    },
    "encoder": {
        "word": {
            "type": "bi_gru",
            "units": 25
        }
    },
    "seq2seq_encoder": {
        "question_0": {
            "type": "bi_gru",
            "encoder_params": {
                "units": 128
            },
            "wrapper_params": {}
        },
        "document_0": {
            "type": "bi_gru",
            "encoder_params": {
                "units": 128
            },
            "wrapper_params": {}
        },
        "question_1": {
            "type": "bi_gru",
            "encoder_params": {
                "units": 128
            },
            "wrapper_params": {}
        },
        "document_1": {
            "type": "bi_gru",
            "encoder_params": {
                "units": 128
            },
            "wrapper_params": {}
        },
        "document_final": {
            "type": "bi_gru",
            "encoder_params": {
                "units": 128
            },
            "wrapper_params": {}
        },
        "question_final":{
            "type": "bi_gru",
            "encoder_params": {
                "units": 128
            },
            "wrapper_params": {
                "merge_mode": "None"
            }
        }
    },
    "optimizer": {
        "type": "adam",
        "learning_rate": 0.0005
    },
    "gradient_clipping": {
        "type": "clip_by_norm",
        "value": 10
    },
    "patience": 5,
    "num_epochs": 10,
    "validation_files": ["/net/efs/data/dlfa/who_did_what/processed/strict/val.tsv"],
    "train_files": ["/net/efs/data/dlfa/who_did_what/processed/strict/train.tsv"]
}
